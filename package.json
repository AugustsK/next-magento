{
  "name": "next-magento",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint": "next lint",
    "lint": "next lint && yarn run prettier --check",
    "prettier": "prettier --loglevel warn --ignore-path .gitignore './**/src/**/*.@(css|graphql|js|ts|jsx|tsx)' '*.js'",
    "format": "yarn run -s prettier --write && yarn run eslint --fix"
  },
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.1.0",
    "@apollo/client": "^3.6.9",
    "@magento/pagebuilder": "^7.4.1",
    "@magento/peregrine": "^12.5.1",
    "@magento/venia-ui": "^9.6.0",
    "@tailwindcss/forms": "^0.5.2",
    "apollo3-cache-persist": "^0.14.1",
    "graphql": "^16.6.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "http-proxy-middleware": "^2.0.6",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  },
  "alias": {
    "@/app": "src",
    "@/types": "@types",
    "@/layouts": "src/layouts",
    "@/components": "src/components",
    "@/styles": "styles",
    "@/hooks": "src/hooks",
    "@/queries": "src/queries"
  }
}
